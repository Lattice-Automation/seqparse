{
  "name": "seqparse",
  "version": "0.1.0",
  "description": "Parse sequence files (GenBank, FASTA, SnapGene, SBOL) and accession IDs (NCBI, iGEM) to a common format",
  "main": "src/index.ts",
  "scripts": {
    "build": "rm -rf ./dist && webpack",
    "fix": "prettier ./src/** --write && eslint src --ext ts,tsx --fix",
    "lint": "prettier ./src/** --check && eslint src --ext ts,tsx --quiet",
    "major": "./config/release.sh major",
    "minor": "./config/release.sh minor",
    "patch": "./config/release.sh patch",
    "test": "jest"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lattice-Automation/seqparse.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Lattice-Automation/seqparse/issues"
  },
  "homepage": "https://github.com/Lattice-Automation/seqparse#readme",
  "dependencies": {
    "buffer": "^6.0.3",
    "bufferpack": "0.0.6",
    "string_decoder": "^1.3.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.15",
    "@types/xml2js": "^0.4.9",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "css-loader": "^3.6.0",
    "eslint": "^8.8.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "html-webpack-plugin": "^4.5.2",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^29.0.2",
    "jest-fetch-mock": "^3.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "path-browserify": "^1.0.1",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "source-map-loader": "^1.1.3",
